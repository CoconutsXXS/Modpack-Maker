{
  "name": "modpack-maker",
  "version": "0.0.3",
  "email": "axel.ohresser47@gmail.com",
  "description": "Lightweight app to create and launch modpack. Easy debuging that let you combine hundred of mods together while keeping the game fluid.",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "build": "electron-builder",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "dist": "electron-builder"
  },
  "author": "Coconuts XXS",
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.9.0",
    "@electron-forge/maker-deb": "^7.9.0",
    "@electron-forge/maker-dmg": "^7.9.0",
    "@electron-forge/maker-rpm": "^7.9.0",
    "@electron-forge/maker-squirrel": "^7.9.0",
    "@electron-forge/maker-zip": "^7.9.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.9.0",
    "@electron-forge/plugin-fuses": "^7.9.0",
    "@electron/fuses": "^1.8.0",
    "electron": "^38.2.1",
    "electron-rebuild": "^3.2.9"
  },
  "dependencies": {
    "@ghostery/adblocker-electron": "^2.12.3",
    "@msgpack/msgpack": "^3.1.2",
    "@nut-tree-fork/nut-js": "^4.2.6",
    "@spyglassmc/mcfunction": "^0.2.38",
    "@xmcl/game-data": "^1.2.6",
    "@xmcl/world": "^1.2.0",
    "buffer-to-data-url": "^1.0.0",
    "chokidar": "^4.0.3",
    "cross-fetch": "^4.1.0",
    "electron-squirrel-startup": "^1.0.1",
    "event-stream": "^4.0.1",
    "extract-zip": "^2.0.1",
    "fast-xml-parser": "^5.3.0",
    "grape-electron-dl": "^1.0.1-3.0.0",
    "lodash": "^4.17.21",
    "minecraft-data": "^3.99.1",
    "minecraft-launcher-core": "^3.18.2",
    "node-stream-zip": "^1.15.0",
    "node-window-manager": "^2.2.4",
    "postprocessing": "^6.37.8",
    "prismarine-nbt": "^2.7.0",
    "realism-effects": "^1.1.2",
    "similarity": "^1.2.1",
    "three": "^0.180.0",
    "toml": "^3.0.0",
    "unzipit": "^1.4.3"
  },
  "build": {
    "appId": "fr.coconuts.modpackmaker",
    "productName": "ModpackMaker",
    "directories": {
      "output": "dist"
    },
    "files": [
      "!immersion-intro"
    ],
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "icon/icon.icns"
    },
    "win": {
      "target": ["nsis", "zip"],
      "icon": "icon/icon.png"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "icon/icon.png"
    }
  }
}
