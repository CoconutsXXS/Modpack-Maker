{
  "name": "modpack-maker",
  "version": "0.0.3",
  "description": "Lightweight app to create and launch modpack. Easy debuging that let you combine hundred of mods together while keeping the game fluid.",
  "main": "main.js",
  "scripts": {
    "dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "build:ui": "vite build",
    "build": "vite build && electron-builder",
    "start": "vite build && electron ."
  },
  "author": {
    "name": "Coconuts XXS",
    "email": "axel.ohresser47@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.8.3",
    "@electron-forge/maker-deb": "^7.9.0",
    "@electron-forge/maker-dmg": "^7.9.0",
    "@electron-forge/maker-rpm": "^7.9.0",
    "@electron-forge/maker-squirrel": "^7.9.0",
    "@electron-forge/maker-zip": "^7.9.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.9.0",
    "@electron-forge/plugin-fuses": "^7.9.0",
    "@electron/fuses": "^1.8.0",
    "@electron/rebuild": "^3.7.2",
    "electron": "^38.3.0",
    "electron-builder": "^26.0.12"
  },
  "dependencies": {
    "@codemirror/commands": "^6.9.0",
    "@codemirror/lang-java": "^6.0.2",
    "@codemirror/lang-json": "^6.0.2",
    "@codemirror/language": "^6.11.3",
    "@codemirror/legacy-modes": "^6.5.2",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@codemirror/view": "^6.38.6",
    "@dimforge/rapier3d-compat": "^0.19.2",
    "@ghostery/adblocker-electron": "^2.12.3",
    "@meza/curseforge-fingerprint": "^1.1.42",
    "@msgpack/msgpack": "^3.1.2",
    "@rollup/plugin-commonjs": "^28.0.8",
    "@spyglassmc/mcfunction": "^0.2.38",
    "@xmcl/core": "^2.15.1",
    "@xmcl/game-data": "^1.2.6",
    "@xmcl/installer": "^6.1.2",
    "@xmcl/world": "^1.2.0",
    "buffer-to-data-url": "^1.0.0",
    "canvas-to-buffer": "^4.1.2",
    "chokidar": "^4.0.3",
    "codemirror": "^6.0.2",
    "comment-json": "^4.4.1",
    "copy-to-clipboard": "^3.3.3",
    "cross-fetch": "^4.1.0",
    "electron-squirrel-startup": "^1.0.1",
    "event-stream": "^4.0.1",
    "extract-zip": "^2.0.1",
    "fast-xml-parser": "^5.3.0",
    "grape-electron-dl": "^1.0.1-3.0.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "minecraft-data": "^3.99.1",
    "minecraft-launcher-core": "^3.18.2",
    "msmc": "^5.0.5",
    "node-stream-zip": "^1.15.0",
    "node-window-manager": "^2.2.4",
    "postprocessing": "^6.37.8",
    "prismarine-nbt": "^2.7.0",
    "similarity": "^1.2.1",
    "tar": "^7.5.1",
    "three": "^0.180.0",
    "three-csg-ts": "^3.2.0",
    "three-mesh-bvh": "^0.9.1",
    "toml": "^3.0.0",
    "tweakpane": "^4.0.5",
    "unzipit": "^1.4.3",
    "unzipper": "^0.12.3",
    "vite": "^7.1.11",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0",
    "wrappy": "^1.0.2",
    "youtube-captions-scraper": "^2.0.3"
  },
  "build": {
    "appId": "fr.coconuts.modpackmaker",
    "productName": "ModpackMaker",
    "directories": {
      "output": "release",
      "buildResources": "build"
    },
    "files": [
      "!immersion-intro",
      "!Firefox Extension",
      "!Firefox\\ Extension",
      "dist/**/*",
      "main.js",
      "*.js",
      "renderer/**/*"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "CoconutsXXS",
        "repo": "Modpack-Maker"
      }
    ],
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "icon/icon.icns"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "artifactName": "${productName}-${version}-${arch}.${ext}",
      "icon": "icon/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    },
    "portable": {
      "artifactName": "${productName}-${version}-portable.${ext}",
      "requestExecutionLevel": "user"
    },
    "linux": {
      "maintainer": "CoconutsXXS <axel.ohresser47@gmail.com>",
      "category": "Utility",
      "target": [
        "deb"
      ],
      "icon": "icon/icon.png"
    },
    "deb": {
      "priority": "optional",
      "compression": "xz",
      "depends": [
        "libappindicator3-1",
        "gconf2",
        "libnotify4"
      ]
    }
  },
  "postinstall": "electron-builder install-app-deps"
}
